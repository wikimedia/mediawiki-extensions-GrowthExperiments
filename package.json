		"@wdio/devtools-service": "7.19.7",
		"@wdio/junit-reporter": "7.16.6",
		"@wdio/local-runner": "7.19.7",
		"@wdio/mocha-framework": "7.16.6",
		"@wdio/spec-reporter": "7.19.7",
		"@wikimedia/codex": "0.8.0",
		"api-testing": "1.5.0",
